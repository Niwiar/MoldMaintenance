<body class="d-flex justify-content-center align-items-center" style="min-height: 100vh; background: #eeeeee">
  <div class="container bg-transparent">
    <div class="row d-flex justify-content-center">
      <div class="col-md-10 col-lg-8 col-xl-8 col-xxl-8">
        <div class="row my-3">
          <div class="col d-flex justify-content-center">
            <img src="img/logo.png" style="width: 350px" />
          </div>
        </div>

        <div class="card mb-5 mt-4">
          <div class="card-body bg-light px-2 py-3 box-shadow">


            <div class="row text-center my-2">
              <h1 id="login-logo" class="d-none d-lg-block fw-bold">Mold Maintenance System</h1>
            </div>

            <div class="row p-3">
              <div class="col">
                <form class="text-center d-flex flex-column align-items-center login-form" action="/user/login"
                  method="post">
                  <h1 class="mb-4">
                    <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 -64 640 640" width="1em" height="1em"
                      fill="currentColor" class="login-icon">
